    <!-- Faction Abilities -->

    <string name="ability_vampire_name">Vampire</string>
    <string name="ability_vampire_description">
        When a &term_unit; &term_trigger_death; by this &term_unit;, &term_action_put_in_play; another
        &group_stillblood_name; from your &term_deck; with a &term_summon_cost; less than
        the &term_sacrifice_value; of the &term_unit; that &term_trigger_death_pt;.
    </string>

    <string name="ability_nosferatu_name">Nosferatu</string>
    <string name="ability_nosferatu_description">
        When this &term_unit; &term_trigger_takes_damage; in combat, subtract any &term_trigger_damage_dealt;
        by this &term_unit; from the &term_trigger_takes_damage_part; by this &term_unit;.
    </string>

    <string name="ability_wight_name">Wight</string>
    <string name="ability_wight_description">
        When this &term_unit; &term_trigger_death; set it aside for one &term_turn;. At the end
        of your next &term_turn; shuffle it back into your &term_deck;.
    </string>

    <string name="ability_zombie_name">Zombie</string>
    <string name="ability_zombie_description">
        This &term_unit; is immune to non-damage effects.
    </string>

    <string name="ability_lycanthrope_name">Lycanthrope</string>
    <string name="ability_lycanthrope_description">
        When this &term_unit; &term_trigger_kills; another &term_unit; in combat, give this &term_unit; a +1/0 &term_counter;.
    </string>

    <string name="ability_animated_name">Animated</string>
    <string name="ability_animated_description">
        When you &term_trigger_pay_to_summon; this &term_unit; treat all &term_costs; as &term_colorless;.
    </string>

    <string name="ability_totemic_spirit_name">Totemic Spirit</string>
    <string name="ability_totemic_spirit_description">
        Before attacking you may switch your opponents &term_front_row; and &term_back_row;.
    </string>

    <string name="ability_witch_bound_name">Witch Bound</string>
    <string name="ability_witch_bound_description">
        This &term_unit; is immune to damage. If any effect causes it to switch positions, this &term_unit; &term_trigger_dies;.
    </string>

    <string name="ability_warlock_name">Warlock/Witch</string>
    <string name="ability_warlock_description">
        You may &term_action_tap; this &term_unit; and pay its &term_sacrifice_value; to search your
        &term_deck; for a &term_subservient; &group_piercers_member; &term_unit; and put it into
        your &term_hand;.
    </string>

    <string name="ability_elemental_name">Elemental</string>
    <string name="ability_elemental_description">
        (Add one additional &term_strength; or &term_life; per &term_mana_ball;.
        This &term_unit; may not have &term_colorless; &term_costs;).
    </string>

    <string name="ability_bound_demon_name">Bound Demon</string>
    <string name="ability_bound_demon_description">
        This &term_unit; takes no damage from &term_subservient; &term_unit;s.
    </string>

    <string name="ability_magical_construct_name">Magical Construct</string>
    <string name="ability_magical_construct_description">
        This &term_unit; may only be &term_summoned; by a &faction_warlocks_member; using its
        ability to draw it from their &term_deck; and then paying the &term_mana_ball;
        difference between the &faction_warlocks_member; ability and the &term_costs; of this
        &term_unit;.
    </string>

    <string name="ability_horned_one_name">Horned One</string>
    <string name="ability_horned_one_description">
        If this &term_unit; would damage another &term_unit; in combat, but that &term_unit; is immune to damage, that &term_unit; instantly &term_trigger_dies;.
    </string>

    <string name="ability_wish_granter_name">Wish Granter</string>
    <string name="ability_wish_granter_description">
        When this &term_unit; is &term_summoned;, search your &term_deck; for any &term_unit; and put it
        directly into play. When this &term_unit; leaves play the target &term_unit; is &term_trigger_removed_from_play;.
    </string>

    <string name="ability_unseelie_name">Unseelie</string>
    <string name="ability_unseelie_description">
        When this &term_unit; &term_trigger_kills; another &term_unit;, look at your opponent\'s &term_hand;.
    </string>

    <string name="ability_troll_name">Troll</string>
    <string name="ability_troll_description">
        When this &term_unit; &term_trigger_kills; another &term_unit; in combat, give this &term_unit; a 0/+1 &term_counter;.
    </string>

    <string name="ability_great_old_one_name">Great Old One</string>
    <string name="ability_great_old_one_description">
        This &term_unit; can be &term_summoned; from the &term_graveyard; as if it were in your
        &term_hand;.
    </string>

    <string name="ability_cultist_name">Cultist</string>
    <string name="ability_cultist_description">
        When this &term_unit; is &term_summoned; it gains one ability of your choice from an adjacent
        &group_old_ones_member;.
    </string>

    <string name="ability_ancient_race_name">Ancient Race</string>
    <string name="ability_ancient_race_description">
        Once per &term_turn; you may search your &term_deck; for another
        &faction_ancient_race_member; and put it into your &term_hand;.
    </string>

    <string name="ability_yellow_mask_name">Yellow Mask</string>
    <string name="ability_yellow_mask_description">
        When this &term_unit; attacks, if there is a &term_unit; in the &term_back_row;, you may choose to
        have the &term_unit; in the &term_back_row; take damage instead of this &term_unit;.
    </string>

    <string name="ability_architect_name">Architect</string>
    <string name="ability_architect_description">
        You may &term_trigger_sacrifice; a &faction_hellspawn_member; to prevent all damage
        to this &term_unit; this turn.
    </string>

    <string name="ability_doppelganger_name">Doppelganger</string>
    <string name="ability_doppelganger_description">
        Copy all of the abilities of an enemy &term_unit; in play when this &term_unit; is &term_summoned;.
    </string>

    <string name="ability_marked_name">Marked</string>
    <string name="ability_marked_description">
        Once this &term_unit; has been &term_summoned;, it may returned to your &term_hand; at
        any time, and returned to play during a subsequent &term_turn; without paying its
        &term_costs;.
